{
    "name": "crimzen",
    "displayName": "CrimZen",
    "description": "Color theme that mixes a light theme for the coding area with a dark theme for all other regions. Prioritizes legibility and reduced color harshness.",
    "version": "3.5.0",
    "publisher": "EricSiron",
    "author": {
        "name": "Eric Siron"
    },
    "bugs": {
        "url": "https://github.com/ejsiron/CrimZen/issues"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.96.2"
    },
    "galleryBanner": {
        "color": "#873636",
        "theme": "dark"
    },
    "icon": "CrimZen.png",
    "categories": [
        "Themes"
    ],
    "keywords": [
        "Theme",
        "Light Theme",
        "CrimZen"
    ],
    "contributes": {
        "themes": [
            {
                "uiTheme": "vs",
                "label": "CrimZen",
                "path": "./themes/CrimZen-color-theme.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ejsiron/CrimZen",
        "directory": "main"
    },
    "configurationDefaults": {
        "[languageId": {
            "editor.semanticHighlighting.enabled": true
        }
    }
}